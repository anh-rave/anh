{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "eslint:recommended",
        "plugin:react/jsx-runtime",
        "plugin:react/recommended",
        "prettier"
    ],
    "rules": {
        "react/prop-types": "off"
    }
}
